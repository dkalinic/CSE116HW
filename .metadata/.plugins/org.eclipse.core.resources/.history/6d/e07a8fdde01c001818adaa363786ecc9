package edu.buffalo.cse116;

public class WinState {

	private Boolean won;
	private String team;

public WinState(Boolean State, String alliance) {
	
	this.won = State;
	this.team = alliance;
}

public Boolean getState() {
	
	return this.won;
}

public String getTeam() {
	
	return this.team;
}

public void setState(Boolean state) {
	
	this.won=state;
}

public void setTeam(String teams) {
	
	this.team=teams;
}


}


